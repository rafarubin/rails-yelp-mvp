<h1>New Restaurant</h1>

<div>
  <%= simple_form_for(@restaurant) do |f|%>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.submit "Create a new restaurant" %>
  <% end %>
</div>

<%= link_to 'Back to list', restaurants_path %>
